{
  "name": "yap",
  "version": "1.0.0",
  "type": "module",
  "module": "index.ts",
  "scripts": {
    "yap": "bun run ./src/index.ts --",
    "lint": "oxlint",
    "typecheck": "tsgo --noEmit",
    "format": "oxfmt . --write",
    "compile": "bun build ./src/index.ts --compile --minify --sourcemap --outfile yap && chmod +x ./yap"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.35.0",
    "goke": "^6.1.3",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "@typescript/native-preview": "^7.0.0-dev.20260213.1",
    "oxfmt": "^0.32.0",
    "oxlint": "^1.47.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
